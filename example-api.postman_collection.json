			"name": "Testes",
			"item": [
				{
					"name": "CRIAÇÃO DE NOVO USUARIO",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test ('Deve retornar 201 e mensagem de Usuario criado quando passivo de usuario ser criado', function () {\r",
									"    pm.response.to.have.status(201);\r",
									"    pm.response.to.be.withBody;\r",
									"    pm.response.to.be.json;\r",
									"\r",
									"    const responseJson = pm.response.json();\r",
									"    pm.expect(responseJson).to.be.an('object');\r",
									"\r",
									"    pm.expect(responseJson.usuarioCriado).to.be.an('number');\r",
									"    pm.expect(responseJson.email).to.be.an('string');\r",
									"\r",
									"    \r",
									"})"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"email\" : \"vbarrilhas.dev@gmail.com\",\r\n    \"senha\" : \"123456\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "localhost:3000/usuarios/cadastro",
							"host": [
