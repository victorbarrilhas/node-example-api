				},
				{
					"name": "CRIAÇÃO DE USUARIO DUPLICADO",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test ('Deve retornar 409 e mensagem de Usuario já cadastrado quando for duplicado', function () {\r",
									"    pm.response.to.have.status(409);\r",
									"    pm.response.to.be.withBody;\r",
									"    pm.response.to.be.json;\r",
									"\r",
									"    const responseJson = pm.response.json();\r",
									"    pm.expect(responseJson).to.be.an('object');\r",
									"\r",
									"    pm.expect(responseJson.mensagem).to.be.an('string');\r",
									"    pm.expect(responseJson.mensagem).to.be.equal('Usuário já cadastrado');\r",
									"    \r",
									"\r",
									"    \r",
									"})"
								],
